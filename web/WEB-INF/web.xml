<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BoatsAddServlet</servlet-name>
        <servlet-class>BoatsAddServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BoatsEditServlet</servlet-name>
        <servlet-class>BoatsEditServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BoatsDelServlet</servlet-name>
        <servlet-class>BoatsDelServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GeoAddServlet</servlet-name>
        <servlet-class>GeoAddServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GeoDelServlet</servlet-name>
        <servlet-class>GeoDelServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GeoEditServlet</servlet-name>
        <servlet-class>GeoEditServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlertAddServlet</servlet-name>
        <servlet-class>AlertAddServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlertDelServlet</servlet-name>
        <servlet-class>AlertDelServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlertEditServlet</servlet-name>
        <servlet-class>AlertEditServlet</servlet-class>
    </servlet>



    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BoatsAddServlet</servlet-name>
        <url-pattern>/addboats</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BoatsEditServlet</servlet-name>
        <url-pattern>/editboats</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BoatsDelServlet</servlet-name>
        <url-pattern>/delboats</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GeoAddServlet</servlet-name>
        <url-pattern>/addgeo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GeoDelServlet</servlet-name>
        <url-pattern>/delgeo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GeoEditServlet</servlet-name>
        <url-pattern>/editgeo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlertAddServlet</servlet-name>
        <url-pattern>/addalert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlertDelServlet</servlet-name>
        <url-pattern>/delalert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlertEditServlet</servlet-name>
        <url-pattern>/editalert</url-pattern>
    </servlet-mapping>
</web-app>